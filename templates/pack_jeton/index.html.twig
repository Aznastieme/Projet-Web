{% extends 'base.html.twig' %}

{% block title %}Liste des pack jetons{% endblock %}


{% block css %} 
<link rel="stylesheet" href="http://cdn.datatables.net/1.10.20/css/jquery.dataTables.min.css">
{% endblock %}

{% block js %} 
<script src="http://cdn.datatables.net/1.10.20/js/jquery.dataTables.min.js"></script>
<script>
$(document).ready( function () {
    $('#table').DataTable();
} );
</script>
{% endblock %}

{% block body %}


<style>
    .SEP{  display: block;
            width: 90px;
            height: 40px;
            font-size:15px;
            }
    #texteButton{color: #FFFFFF; text-align: center;}
</style>

    <h1>Liste des pack jetons</h1>

    <table id="table" class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nbjetons</th>
                <th>Description</th>
                <th>Prix</th>
                <th>photo</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for pack_jeton in pack_jetons %}
            <tr>
                <td>{{ pack_jeton.id }}</td>
                <td>{{ pack_jeton.nbjetons }}</td>
                <td>{{ pack_jeton.description }}</td>
                <td>{{ pack_jeton.prix }}</td>
                <td> <img width="200" height="200" src="
                data:image/jpeg;base64,
                " /> </td>
                
                <td>
                    <a class="btn btn-primary SEP" class="nav-link" href="{{ path('pack_jeton_show', {'id': pack_jeton.id}) }}"><div id = "texteButton">voir</div></a>
                    {% if is_granted('ROLE_MODO') %}
                    <a class="btn btn-warning SEP" class="nav-link" href="{{ path('pack_jeton_edit', {'id': pack_jeton.id}) }}"><div id = "texteButton">modifier</div></a>
                    {{ include('pack_jeton/_delete_form.html.twig') }}
                 {% endif %}
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    {% if is_granted('ROLE_MODO') %}
    <a class='btn btn-primary' href="{{ path('pack_jeton_new') }}">Ajouter un pack jetons</a>
    {% endif %}
{% endblock %}
